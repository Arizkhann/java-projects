<!DOCTYPE html>
<html>
<head>
	<script>
		function show(){
			var result="";
			var obj=new Date();
			var month=obj.getMonth();
			month=month +1;
			
			result+= "today's date is " +obj.getDate()+"-" + month +"-"+obj.getFullYear() +"<br/>";
	//		result+= "month is "+month + "<br/>";
		//	result+= "year is "+obj.getFullYear() + "<br/>";
			//result+= "Hours is "+obj.getHours() + "<br/>";
			//result+= "minute is "+obj.getMinutes() + "<br/>";
			//result+= "second is "+obj.getSeconds() + "<br/>";
			document.getElementById("result").innerHTML=result;
			
		}
		
		
		
		
		
	</script>
	
<meta charset="ISO-8859-1">
<title>Insert title here</title>
</head>
<body   onload="show()">
	
	<span id="result"></span>
	

</body>
</html>